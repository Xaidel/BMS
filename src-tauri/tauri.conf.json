{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "bms",
  "version": "0.1.0",
  "identifier": "com.bms.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Barangay Management System",
        "fullscreen": true,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
  "active": true,
  "targets": "all",
  "icon": [
    "icons/new_logo_small.png",
    "icons/new_logo_small.png",
    "icons/new_logo_small.png",
    "icons/new_logo_small.png",
    "icons/new_logo_small.png"
  ]
}
}
